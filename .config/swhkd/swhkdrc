super + Return
    kitty

super + shift + {e,r}
    bspc {quit,wm -r}

super + d
    wofi --show drun

super + {_,shift + }{1-9,0}
	bspc {desktop -f,node -d} '^{1-9,10}'

super + {alt,shift} + q
	bspc node -{k,c}

super + x
    betterlockscreen -l

super + shift + x
    betterlockscreen -l & systemctl suspend

@Print
    scrot ~/screenshots/scr.png

shift + @Print
    scrot -s ~/screenshots/scr.png

super + e
    pkill -USR1 -x sxhkd

super + {f, t, space}
    bspc node -t {fullscreen, tiled, floating}
